# cbnf's self description

comment {
    '#' text (EOL | EOF)
}
syntax { 
    rule syntax | nil
}
rule {
    ident '{' list '}'
}
list {
    term (list | nil)
}
term {
    literal | char | ident | group | "nil"
}
group {
    '(' list ')'
}
literal {
    '"' text '"'
}
char {
    '\'' char '\''
}

text {}
ident {}
EOL {}
EOF {}
