# cbnf's self description

comment {
    '#' $text ($EOL | $EOF)
}
syntax { 
    rule syntax | nil
}
rule {
    ($ident | meta) '{' list '}' | meta ';'
}
list {
    term (list | nil)
}
term {
    literal | char | $ident | meta | group | "nil"
}
meta {
    '$' $ident
}
group {
    '(' list ')'
}
literal {
    '"' $text '"'
}
char {
    '\'' $char '\''
}


$text;
$char;
$ident;
$EOL;
$EOF;

